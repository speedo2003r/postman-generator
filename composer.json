{
    "name": "laravelgenerators/postman-generator",
    "description": "Automatically generates a Postman Collection JSON from a Laravel API project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel Generators"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelGenerators\\PostmanGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelGenerators\\PostmanGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelGenerators\\PostmanGenerator\\PostmanGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}